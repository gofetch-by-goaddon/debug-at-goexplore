{
  "update_config": {
    "klass": "webshops",
    "query": {
      "company_id": "{{ company_id }}",
      "_id": "{{ _id }}"
    },
    "document": {
      "config": "{{ config | json }}"
    }
  },
  "reset_got_at_attr": {
    "klass": "webshops",
    "query": {
      "company_id": "{{ company_id }}",
      "_id": "{{ _id }}"
    },
    "document": "{{ document | json }}"
  },
  "toggle_active": {
    "klass": "webshops",
    "query": {
      "company_id": "{{ company_id }}",
      "_id": "{{ _id }}"
    },
    "document": {
      "active": "{{ active | bool }}"
    }
  }
}