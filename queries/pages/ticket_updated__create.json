{
  "superusers": {
    "klass": "users",
    "query": {
      "app_id": "{{ app_id }}",
      "superuser": true
    },
    "projection": {
      "_id": 1,
      "email": 1,
      "superuser_level": 1,
      "ticket_relay_ids": 1,
      "closed_ticket_relay_ids": 1,
      "notification_relay_ids": 1
    }
  }
}